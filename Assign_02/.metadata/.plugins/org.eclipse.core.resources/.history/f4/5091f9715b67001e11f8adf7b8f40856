package com.app.geometry;

public class Point2D {
	int x;
	int y;

	Point2D(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public String getDetails() {
		return Integer.toString(this.x, this.y);

	}

	public boolean isEqual(p1,p2) {
		if (this.x == this.y) 
		{
			return true;
		} else 
		{
			return false;
		}

	}
	
	public 

}
