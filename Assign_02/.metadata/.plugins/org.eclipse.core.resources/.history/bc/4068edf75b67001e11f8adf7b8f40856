package com.app.geometry;

public class Point2D {
	int x;
	int y;

	Point2D(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public String getDetails(Point2D p1,Point2D p2) {
		return (Integer.toString(p1.x,p1.y,p2.x,p2.y);

	}

	public boolean isEqual(Point2D p1,Point2D p2) {
		if (p1.x == p2.x && p1.y==p2.y) 
		{
			return true;
		} else 
		{
			return false;
		}

	}
	
	public 

}
