package com.sunbeam.person;

public class Main {
	public static void main(String[] args) {
		Manager m=new Manager();
		m.accept();
		m.display();
		Generic_Box<Employee> GBE=new Generic_Box<Employee>();
		GBE.set(m);
		GBE.get();
		GBE.getSalary();
		
		Salesman m=new Salesman();
		m.accept();
		m.display();
		Generic_Box<Employee> GBE=new Generic_Box<Employee>();
		GBE.set(m);
		GBE.get();
		GBE.getSalary();
		
		
		
	}

}
